    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "eslint": "^8.4.2",
        "typescript": "^5.1.0",
        "jszip": "^3.10.1",
        "strip-json-comments": "^3.1.1"
    },
    "dependencies": {
        "@minecraft/server": "^1.4.0-beta.1.20.10-preview.24",
        "@minecraft/server-ui": "^1.0.0"
